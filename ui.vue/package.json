{
  "name": "vue-aem-spa",
  "version": "1.0.0",
  "description": "Sample AEM spa + vue + bootstrap",
  "main": "index.js",
  "keywords": [
    "vue",
    "spa",
    "aem",
    "bootstrap"
  ],
  "author": "Gustavo Doriguetto",
  "license": "MIT",
  "scripts": {
    "serve": "cross-env NODE_ENV=development && vue-cli-service serve",
    "build": "cross-env NODE_ENV=production && vue-cli-service build && clientlib --verbose",
    "test:unit": "cross-env NODE_ENV=test && vue-cli-service test:unit",
    "lint": "cross-env NODE_ENV=test && vue-cli-service lint"
  },
  "devDependencies": {
    "@adobe/aem-spa-page-model-manager": "^1.2.3",
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@vue/cli-plugin-eslint": "^3.12.1",
    "@vue/cli-plugin-unit-jest": "^4.5.8",
    "@vue/cli-service": "^4.5.8",
    "@vue/test-utils": "^1.1.0",
    "aem-clientlib-generator": "^1.7.5",
    "axios": "^0.19.2",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "cross-env": "^5.2.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.2.3",
    "i": "^0.3.6",
    "jest-sonar-reporter": "^2.0.0",
    "merge": "^1.2.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^10.0.5",
    "vue": "^2.6.12",
    "vue-jest": "^3.0.7",
    "vue-router": "^3.4.7",
    "vue-spa-component-mapping": "^1.0.3",
    "vue-template-compiler": "^2.6.12",
    "vuex": "^3.5.1"
  },
  "jestSonar": {
    "reportPath": "target/ui-reports",
    "reportFile": "ui-test-report.xml",
    "indent": 4
  },
  "proxy": "http://localhost:4502"
}
